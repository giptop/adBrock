{
  "manifest_version": 3,
  "name": "Google Ads Blocker",
  "version": "1.0",
  "description": "Google広告（doubleclick, googlesyndication など）をブロックする拡張",
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback"],
  "host_permissions": ["<all_urls>"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
